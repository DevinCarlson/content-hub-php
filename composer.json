{
    "name": "acquia/content-service-client-php",
    "type": "library",
    "description": "A PHP Client library for consuming the Content Services API.",
    "homepage": "https://github.com/acquia/content-service-client-php",
    "license": "MIT",
    "authors": [
        {
            "name": "See Contributors",
            "homepage": "https://github.com/acquia/content-service-client-php/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/acquia/content-service-client-php/issues"
    },
    "require": {
        "php": ">=5.4.0",
        "crell/api-problem": "~1.0",
        "guzzlehttp/guzzle": "~5.0",
        "acquia/http-hmac-php": "dev-master"
    },
    "require-dev": {
        "pdepend/pdepend": "~1.0",
        "phploc/phploc": "~2.0",
        "phpmd/phpmd": "~1.0",
        "phpunit/phpunit": "~3.0",
        "scrutinizer/ocular": "~1.0",
        "sebastian/phpcpd": "~2.0"
    },
    "autoload": {
    	"psr-4": {
            "Acquia\\ContentServicesClient\\": "src/"
    	}
    },
    "autoload-dev": {
    	"psr-4": {
            "Acquia\\ContentServicesClient\\": "test/"
    	}
    }
}
